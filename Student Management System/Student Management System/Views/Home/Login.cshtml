@{
}
@model Student_Management_System.Models.Entities.Student;
<form method="post">
    <div class="mb-3">
        <label for="email" class="form-label">Email address</label>
        <input type="email" class="form-control" id="email" aria-describedby="emailHelp" asp-for="EmailId">
        <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
        <label for="pass" class="form-label">Password</label>
        <input type="password" class="form-control" id="pass" asp-for="MobileNo">
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>